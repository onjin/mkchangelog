[GENERAL]
output = CHANGELOG.md
template = my_markdown.jinja2
commit_limit = 100
unreleased = True
unreleased_version = Unreleased
hide_empty_releases = False
changelog_title = Changelog
commit_types_list = fix,feat,refactor
commit_type_default_priority = 10
tag_prefix = v
ignore_revs = 7cbb5e69b2984bc2c69434e49582bb84e2e31df6

[commit_types]
build = Build
chore = Chore
ci = CI
dev = Dev
docs = Docs
feat = Features
fix = Fixes
perf = Performance
refactor = Refactors
style = Style
test = Test
translations = Translations

[commit_types_priorities]
feat = 40
fix = 30
refactor = 20
